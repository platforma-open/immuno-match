{
  "name": "@platforma-open/milaboratories.immuno-match",
  "version": "1.0.0",
  "scripts": {
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,vue}\"",
    "build": "turbo run build",
    "build:dev": "env PL_PKG_DEV=local turbo run build",
    "test": "turbo run test --concurrency 1",
