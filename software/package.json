{
  "name": "@platforma-open/milaboratories.immuno-match.software",
  "version": "1.2.3",
  "type": "module",
  "description": "ImmunoMatch software package",
  "scripts": {
    "do-pack": "rm -f *.tgz && pl-pkg build && pnpm pack && mv platforma-open*.tgz package.tgz",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "build": "pl-pkg build",
    "prepublishOnly": "pl-pkg prepublish"
  },
  "block-software": {
    "artifacts": {
      "immuno-match-artifact": {
        "type": "python",
        "registry": "platforma-open",
        "environment": "@platforma-open/milaboratories.runenv-python-3:3.12.10",
        "dependencies": {
          "toolset": "pip",
          "requirements": "requirements.txt"
        },
        "root": "./src"
      }
    },
    "entrypoints": {
      "immuno-match": {
        "binary": {
          "artifact": "immuno-match-artifact",
          "cmd": [
            "python",
            "{pkg}/immuno-match.py"
          ]
        }
      },
      "default-cutoff": {
        "binary": {
          "artifact": "immuno-match-artifact",
          "cmd": [
            "python",
            "{pkg}/default-cutoff.py"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "@platforma-open/milaboratories.runenv-python-3": "catalog:",
    "@platforma-sdk/package-builder": "catalog:"
  }
}
